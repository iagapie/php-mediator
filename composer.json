{
  "name": "iagapie/php-mediator",
  "type": "library",
  "description": "PHP Mediator",
  "keywords": [
    "php",
    "mediator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Agapie",
      "email": "igoragapie@gmail.com"
    }
  ],
  "require": {
    "php": ">= 8.0"
  },
  "require-dev": {
    "doctrine/dbal": "^3.1",
    "psr/container": "^2.0",
    "psr/log": "^1.1",
    "symfony/validator": "^5.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IA\\Mediator\\": "src/"
    }
  }
}
